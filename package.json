{
  "name": "icx-svelte-sapper-prototype",
  "description": "An experiment using Svelte and Sapper as a replacement for AP, focuses on Data Component rendering",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cypress": "^3.4.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-svelte-unit-test": "^1.5.0",
    "polka": "^0.5.0",
    "sapper-store": "0.0.5",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@storybook/svelte": "^5.1.9",
    "@sveltejs/svelte-virtual-list": "^3.0.0",
    "babel-loader": "^8.0.6",
    "less": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.11",
    "svelte": "^3.0.0",
    "svelte-preprocess-less": "^0.4.0"
  }
}
